  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:ci": "react-scripts test --coverage --color --ci --reporters=default --reporters=jest-junit",
    "test:all": "yarn test --coverage --watchAll",
    "storybook": "start-storybook -p 8200 -s public",
