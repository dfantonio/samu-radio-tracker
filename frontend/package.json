    "build:jsdoc": "jsdoc -c jsdoc.json",
    "lint": "eslint src -c .eslintrc.json --ext js,ts",
    "sonar-scanner": "sonar-scanner"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "src/serviceWorker.js",
      "src/index.js",
      "src/Components/FacebookButton"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": -10
      }
    }
  },
